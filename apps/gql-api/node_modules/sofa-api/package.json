{
  "name": "sofa-api",
  "version": "0.16.3",
  "description": "Create REST APIs with GraphQL",
  "sideEffects": false,
  "peerDependencies": {
    "graphql": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@graphql-tools/utils": "^9.2.0",
    "@whatwg-node/fetch": "^0.6.6",
    "@whatwg-node/router": "^0.2.0",
    "ansi-colors": "^4.1.3",
    "openapi-types": "^12.1.0",
    "param-case": "^3.0.4",
    "title-case": "^3.0.3",
    "tslib": "^2.5.0"
  },
  "repository": {
    "type": "git",
    "url": "Urigo/sofa"
  },
  "keywords": [
    "api",
    "rest",
    "graphql",
    "sofa"
  ],
  "author": {
    "name": "Uri Goldshtein",
    "email": "uri.goldshtein@gmail.com",
    "url": "https://github.com/Urigo"
  },
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "typings": "index.d.ts",
  "typescript": {
    "definition": "index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs"
    },
    "./*": {
      "require": "./*.js",
      "import": "./*.mjs"
    },
    "./package.json": "./package.json"
  }
}